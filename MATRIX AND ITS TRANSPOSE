#include<stdio.h>
int main()
{
    int m,n;
    printf("Enter the value of m and n : ");
    scanf("%d%d",&m,&n);
    int a[m][n];
    printf("Enter the 2d array:");
    for (int i=0;i<m;i++)
    {
        for (int j=0;j<n;j++)
        {
            scanf("%d ",&a[i][j]);
        }
       
    }
     printf("array elements are:\n");
    for(int i=0;i<m;i++)
    {
        for(int j=0;j<n;j++)
        {
            printf("%d ",a[i][j]);
        }
        printf("\n");
    }
    {
        printf("Transpose of a number:\n");
        for(int i=0;i<=n-1;i++)
        {
            for(int j=0;j<=m-1;j++)
            {
                printf("%d ",a[j][i]);
            }
            printf("\n");
        }
        return 0;
    }
}


