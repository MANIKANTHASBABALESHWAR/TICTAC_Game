#include<stdio.h>
int main()
{
    int n,sum=0;
    printf("Enter the size of array:");
    scanf("%d",&n);
    int arr1[n];
    for(int i=0;i<=n-1;i++)
    {
        scanf("%d",&arr1[i]);
        if (arr1[i]%2 == 0)
        sum=sum+arr1[i];
    }
    printf("Even Sum of array elements are:%d",sum);
}
