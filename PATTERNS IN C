9.#include<stdio.h>
#include<stdlib.h>
int main()
{
    int n;
    scanf("%d",&n);
    for (int i=n;i>=1;i--)
    {
        for (int j=n-1;j>=i;j--)
        {
            printf(" ");
        }
        for (int j=1;j<=i;j++)
        {
            printf("*");
        }
    
    
    printf("\n");
    }
}
 o/p
4
